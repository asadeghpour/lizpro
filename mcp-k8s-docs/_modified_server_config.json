{
  "mcpServers": {
    "playbook-repo": {
      "name": "playbook-repo",
      "command": "uv",
      "args": [
        "run",
        "--directory",
        "/Users/christopherhay/chris-source/chuk-ai/mcp-servers/chuk-mcp-playbook",
        "chuk-mcp-playbook"
      ],
      "env": {
        "PATH": "/root/mcp-kubernetes/mcp-venv/bin:/root/.local/share/../bin:/sbin:/usr/sbin:/usr/local/sbin:/root/.local/bin:/root/bin:/usr/local/bin:/usr/bin:/bin",
        "PYTHONWARNINGS": "ignore",
        "LOG_LEVEL": "WARNING",
        "CHUK_LOG_LEVEL": "WARNING",
        "MCP_LOG_LEVEL": "WARNING"
      }
    },
    "sqlite": {
      "name": "sqlite",
      "command": "uvx",
      "args": [
        "mcp-server-sqlite",
        "--db-path",
        "test.db"
      ],
      "env": {
        "PATH": "/root/mcp-kubernetes/mcp-venv/bin:/root/.local/share/../bin:/sbin:/usr/sbin:/usr/local/sbin:/root/.local/bin:/root/bin:/usr/local/bin:/usr/bin:/bin",
        "PYTHONWARNINGS": "ignore",
        "LOG_LEVEL": "WARNING",
        "CHUK_LOG_LEVEL": "WARNING",
        "MCP_LOG_LEVEL": "WARNING"
      }
    },
    "echo": {
      "name": "echo",
      "command": "uvx",
      "args": [
        "chuk-mcp-echo",
        "stdio"
      ],
      "env": {
        "PATH": "/root/mcp-kubernetes/mcp-venv/bin:/root/.local/share/../bin:/sbin:/usr/sbin:/usr/local/sbin:/root/.local/bin:/root/bin:/usr/local/bin:/usr/bin:/bin",
        "PYTHONWARNINGS": "ignore",
        "LOG_LEVEL": "WARNING",
        "CHUK_LOG_LEVEL": "WARNING",
        "MCP_LOG_LEVEL": "WARNING"
      }
    },
    "notion": {
      "name": "notion",
      "env": {
        "PATH": "/root/mcp-kubernetes/mcp-venv/bin:/root/.local/share/../bin:/sbin:/usr/sbin:/usr/local/sbin:/root/.local/bin:/root/bin:/usr/local/bin:/usr/bin:/bin"
      },
      "url": "https://mcp.notion.com/mcp"
    },
    "cloudflare_workers": {
      "name": "cloudflare_workers",
      "env": {
        "PATH": "/root/mcp-kubernetes/mcp-venv/bin:/root/.local/share/../bin:/sbin:/usr/sbin:/usr/local/sbin:/root/.local/bin:/root/bin:/usr/local/bin:/usr/bin:/bin"
      },
      "url": "https://bindings.mcp.cloudflare.com/mcp"
    },
    "brave_search": {
      "name": "brave_search",
      "command": "npx",
      "args": [
        "-y",
        "@brave/brave-search-mcp-server"
      ],
      "env": {
        "BRAVE_API_KEY": "${TOKEN:bearer:brave_search}",
        "PATH": "/root/mcp-kubernetes/mcp-venv/bin:/root/.local/share/../bin:/sbin:/usr/sbin:/usr/local/sbin:/root/.local/bin:/root/bin:/usr/local/bin:/usr/bin:/bin",
        "PYTHONWARNINGS": "ignore",
        "LOG_LEVEL": "WARNING",
        "CHUK_LOG_LEVEL": "WARNING",
        "MCP_LOG_LEVEL": "WARNING"
      }
    },
    "monday": {
      "name": "monday",
      "env": {
        "PATH": "/root/mcp-kubernetes/mcp-venv/bin:/root/.local/share/../bin:/sbin:/usr/sbin:/usr/local/sbin:/root/.local/bin:/root/bin:/usr/local/bin:/usr/bin:/bin"
      },
      "url": "https://mcp.monday.com/mcp"
    },
    "playwright": {
      "name": "playwright",
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ],
      "env": {
        "PATH": "/root/mcp-kubernetes/mcp-venv/bin:/root/.local/share/../bin:/sbin:/usr/sbin:/usr/local/sbin:/root/.local/bin:/root/bin:/usr/local/bin:/usr/bin:/bin",
        "PYTHONWARNINGS": "ignore",
        "LOG_LEVEL": "WARNING",
        "CHUK_LOG_LEVEL": "WARNING",
        "MCP_LOG_LEVEL": "WARNING"
      }
    },
    "linkedin": {
      "name": "linkedin",
      "env": {
        "PATH": "/root/mcp-kubernetes/mcp-venv/bin:/root/.local/share/../bin:/sbin:/usr/sbin:/usr/local/sbin:/root/.local/bin:/root/bin:/usr/local/bin:/usr/bin:/bin"
      },
      "url": "https://linkedin.chukai.io/mcp"
    },
    "weather": {
      "name": "weather",
      "env": {
        "PATH": "/root/mcp-kubernetes/mcp-venv/bin:/root/.local/share/../bin:/sbin:/usr/sbin:/usr/local/sbin:/root/.local/bin:/root/bin:/usr/local/bin:/usr/bin:/bin"
      },
      "url": "https://weather.chukai.io/mcp"
    },
    "telnet": {
      "name": "telnet",
      "command": "uvx",
      "args": [
        "chuk-mcp-telnet-client"
      ],
      "env": {
        "PATH": "/root/mcp-kubernetes/mcp-venv/bin:/root/.local/share/../bin:/sbin:/usr/sbin:/usr/local/sbin:/root/.local/bin:/root/bin:/usr/local/bin:/usr/bin:/bin",
        "PYTHONWARNINGS": "ignore",
        "LOG_LEVEL": "WARNING",
        "CHUK_LOG_LEVEL": "WARNING",
        "MCP_LOG_LEVEL": "WARNING"
      }
    }
  },
  "defaultProvider": "openai",
  "defaultModel": "gpt-4",
  "theme": "default",
  "verbose": true,
  "confirmTools": true
}