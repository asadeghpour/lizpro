{
  "mcpServers": {
    "RK2-Manager": {
      "command": "node",
      "args": ["/root/mcp-kubernetes/dist/index.js"],
      "env": {
        "PATH": "/usr/local/bin:/usr/bin:/bin",
        "KUBECONFIG": "/root/.kube/config"
      }
    },
    "K8s-Docs": {
      "command": "node",
      "args": ["/root/mcp-k8s-docs/dist/index.js"]
    }
  }
}
